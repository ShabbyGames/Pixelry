if side = "right" image_xscale = 1;
if action = "neutral" and moving = 0
{
    image_speed = 0;
    image_xscale = -1;
    image_index = 0;
    if side = "left" image_xscale = -1;
    if side = "right" image_xscale = 1;
}
if action = "neutral" and moving = 1
{
    image_speed = 0.3;
    if image_index &lt; 9 image_index = 9;
    if image_index &gt; 13 image_index = 9;
}
if action = "neutral" and inAir = 1
{
    image_speed = 0;
    image_index = 13;
}
if action = "attack"and inAir = 0
{
    image_speed = 0.2
    if lock = 0
    {
        alarm[0] = 15;
        chooseThing = choose(0,1,2);
        switch(chooseThing)
        {
            case 0: image_index = 3;
                break;
            case 1: image_index = 5;
                break;
            case 2: image_index = 7;
                break;
        }
        lock = 1;
    }
    switch(chooseThing)
    {
        case 0: 
            if image_index &lt; 3 image_index = 3;
            if image_index &gt; 4 image_index = 4;
            if image_index = 4 and lock = 1
            {
                if side = "right"
                    hitbox_create(x,y,obj_hitbox,0,1,1,0.5,1,5,"neutral",spr_overlay_attack_sword,true);
                if side = "left"
                    hitbox_create(x,y,obj_hitbox,0,-1,1,0.5,1,5,"neutral",spr_overlay_attack_sword,true);
                lock = 2;
            }
            break;
        case 1:
            if image_index &lt; 5 image_index = 5;
            if image_index &gt; 6 image_index = 6;
            if image_index = 6 and lock = 1
            {
                if side = "right"
                    hitbox_create(x,y,obj_hitbox,2,1,1,0.5,3,5,"neutral",spr_overlay_attack_sword,true);
                if side = "left"
                    hitbox_create(x,y,obj_hitbox,2,-1,1,0.5,3,5,"neutral",spr_overlay_attack_sword,true);
                lock = 2;
            }
            break;
        case 2:
            if image_index &lt; 7 image_index = 7;
